<h1> Update <%= @cat.name %>!</h1>

<form method="POST" action="<%= cat_url(@cat) %>">
    <input type="hidden" name="_method" value="PATCH">
    <label for="cat_name"> Name </label>
    <input type="text" name="cat[name]" id="cat_name" value="<%= @cat.name %>">
    <br>
    <label for="skill_name"> Skill </label>
    <input type="text" name="cat[skill]" id="skill_name" value="<%= @cat.skill %>">
    <br>
    <br>
    <input type="submit" value="Update Cat!">
    
</form>